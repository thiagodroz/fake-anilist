{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/next" },
  "defaultProject": "website",
  "generators": {
    "@nrwl/react": { "application": { "babel": true } },
    "@nrwl/next": {
      "application": { "style": "styled-components", "linter": "eslint" }
    }
  },
  "projects": {
    "website": {
      "root": "apps/website",
      "sourceRoot": "apps/website",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/website",
            "outputPath": "dist/apps/website"
          },
          "configurations": { "production": {} }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": { "buildTarget": "website:build", "dev": true },
          "configurations": {
            "production": {
              "buildTarget": "website:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": { "buildTarget": "website:build:production" }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/website"],
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/website/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "website-e2e": {
      "root": "apps/website-e2e",
      "sourceRoot": "apps/website-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/website-e2e/cypress.json",
            "tsConfig": "apps/website-e2e/tsconfig.e2e.json",
            "devServerTarget": "website:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "website:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/website-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "apollo": {
      "root": "libs/apollo",
      "sourceRoot": "libs/apollo/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/apollo/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/apollo"],
          "options": {
            "jestConfig": "libs/apollo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "workshop-web": {
      "root": "libs/workshop-web",
      "sourceRoot": "libs/workshop-web/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/workshop-web/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/workshop-web"],
          "options": {
            "jestConfig": "libs/workshop-web/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": { "configFolder": "libs/workshop-web/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/workshop-web",
            "config": { "configFolder": "libs/workshop-web/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        }
      }
    }
  }
}
